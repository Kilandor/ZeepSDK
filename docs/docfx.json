{
    "metadata":
    [
        {
            "src":
            [
                {
                    "src": "../ZeepSDK/",
                    "files":
                    [
                        "ZeepSDK.csproj"
                    ]
                }
            ],
            "dest": "api",
            "filter": "filterConfig.yml",
            "includePrivateMembers": false,
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "noRestore": false,
            "namespaceLayout": "flattened",
            "memberLayout": "samePage",
            "allowCompilationErrors": true,
            "properties":
            {
                "TargetFramework": "net472"
            }
        }
    ],
    "build":
    {
        "content":
        [
            {
                "files":
                [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files":
                [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource":
        [
            {
                "files":
                [
                    "images/**"
                ]
            }
        ],
        "output": "_site",
        "globalMetadataFiles":
        [],
        "fileMetadataFiles":
        [],
        "template":
        [
            "default",
            "modern"
        ],
        "postProcessors":
        [],
        "keepFileLink": false,
        "disableGitFeatures": false
    }
}