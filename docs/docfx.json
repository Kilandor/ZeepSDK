{
  "metadata": [
    {
      "src": [
        {
          "src":  "../",
          "files": [
            "ZeepSDK/ZeepSDK.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml" ],
        "exclude": [ "api/index.md"]
      }
    ],
    "output": "_site",
    "markdownEngineName": "dfm",
    "markdownEngineProperties": {
        "enableSourceInfo": false
    }
  }
}